{
    "name": "lekoala/silverstripe-debugbar",
    "description": "DebugBar for SilverStripe CMS",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "debugbar",
        "module",
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LeKoala",
            "email": "thomas@lekoala.be"
        }
    ],
    "require": {
        "php": ">=5.6",
        "silverstripe/framework": "dev-pulls/4.0/fix-mysql-gone-away",
        "silverstripe/siteconfig": "^4.0@dev",
        "silverstripe/admin": "^1.0@dev",
        "maximebf/debugbar": "^1.13",
        "jdorn/sql-formatter": "1.3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "installer-name": "debugbar",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "LeKoala\\DebugBar\\": "code/",
            "LeKoala\\DebugBar\\Test\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-debugbar/issues"
    },
    "scripts": {
        "lint": "vendor/bin/phpcs code/ tests/",
        "lint-clean": "vendor/bin/phpcbf code/ tests/"
    },
    "repositories": [
        {"type": "vcs", "url": "git@github.com:creative-commoners/silverstripe-framework.git"}
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
